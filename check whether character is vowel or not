#include <iostream>
using namespace std;

int main(){
  char ch;
  cout<<"enter the character = ";
  cin>>ch;
  if(ch=='a'||ch=='A'||ch=='e' ||ch=='E'||ch=='i'||ch=='I'||ch=='o' ||ch=='O' || ch=='u' || ch=='U' )
  {
      cout<<"the character is vowel ";
  }
  else
  {
      cout<<"the character is consonant";
  }
  
    
    
    return 0;
}

OUTPUT:
enter the character = A
the character is vowel.
